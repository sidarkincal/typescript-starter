datasource db {
  provider = "sqlite"
}

generator client {
  provider     = "prisma-client"
  output       = "../src/generated/prisma"
  moduleFormat = "cjs"
}

model LabItem {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  createdAt   DateTime @default(now())
}

model User {
  id       Int    @id @default(autoincrement())
  fullname String
  email    String
  password String
}
